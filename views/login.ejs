
<div class="container"  > 
<div class="row mt-5">
  <div class="col-md-6 m-auto">
    <div class="card card-body">
      <h1 class="text-center">
        <img src="/ts.png" style="width:140px;">  
      </h1>
      <h5 class="text-center mb-3"><b>TEK SENG HOLDINGS BERHAD</b></h5>
      <!-- <h1 class="text-center mb-3">Login account </h1> -->
      
      <style>
        
      </style>
      <!-- <img src="/public/img/ts.jpg" width="80" height="80">  -->
      <% include ./partials/messages %>
      <form action="/users/login" method="POST">
        
        <div class="form-group">
          <label for="email"><b>Email</b></label>
          <input
            type="email"
            id="email"
            name="email"
            class="form-control"
            placeholder="Enter Email"
          />
        </div>
        <div class="form-group">
          <label for="password"><b>Password</b></label>
          <input
            type="password"
            id="password"
            name="password"
            class="form-control"
            placeholder="Enter Password"
          />
        </div>
        <button type="submit" class="btn btn-primary btn-block"><b>LOGIN</b></button>
      </form>
      <p class="blockquote mt-4">
        No Account? <a href="/users/register">Register</a>
      </p>
    </div>
  </div>
</div>
